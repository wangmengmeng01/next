<!DOCTYPE html>
<html lang="zh_CN" xmlns:th="http://www.thymeleaf.org">
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="provinceid" name="provinceid" th:value="${province.provinceid}" type="hidden">
						
						<!-- 	<div class="form-group">	
								<label class="col-sm-3 control-label">业务省id：</label>
								<div class="col-sm-8">
									<input id="ProvinceID" name="ProvinceID" th:value="ProvinceID" class="form-control" type="text">
								</div>
							</div> -->
							<div class="form-group">	
								<label class="col-sm-3 control-label">省名称：</label>
								<div class="col-sm-8">
									<input id="provincename" name="provincename" th:value="${province.provincename}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">简称：</label>
								<div class="col-sm-8">
									<input id="mininame" name="mininame" th:value="${province.mininame}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">所属大区：</label>
								<div class="col-sm-8">
									<input id="bigarea" name="bigarea" th:value="${province.bigarea}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">新大区编号：</label>
								<div class="col-sm-8">
									<input id="rgId" name="rgId" th:value="${province.rgId}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">order_id：</label>
								<div class="col-sm-8">
									<input id="orderId" name="orderId" th:value="${province.orderId}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">省可维护状态，1不可维护，2可维护：</label>
								<div class="col-sm-8">
									<input id="maintainState" name="maintainState" th:value="${province.maintainState}" class="form-control" type="text">
								</div>
							</div>
																					<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" th:src="@{/js/appjs/sys/province/editProvinceBigarea.js?v=2}">
	</script>
</body>
</html>
