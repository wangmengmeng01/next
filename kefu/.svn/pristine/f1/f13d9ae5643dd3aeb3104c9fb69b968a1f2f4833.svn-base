<!DOCTYPE html>
<html lang="zh_CN" xmlns:th="http://www.thymeleaf.org">
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="id" name="id" th:value="${resourceSafeConfig.id}"  type="hidden">
							<div class="form-group">	
								<label class="col-sm-3 control-label">页面路径：</label>
								<div class="col-sm-8">
									<input id="resourcePath" name="resourcePath" th:value="${resourceSafeConfig.resourcePath}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">安全等级：</label>
								<div class="col-sm-8">
									<input id="safeLevel" name="safeLevel" th:value="${resourceSafeConfig.safeLevel}" class="form-control" type="text">
								</div>
							</div>
																					<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" th:src="@{/js/appjs/system/resourceSafeConfig/edit.js}">
	</script>
</body>
</html>
