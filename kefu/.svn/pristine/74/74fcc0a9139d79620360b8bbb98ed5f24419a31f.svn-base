<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yunda.base.bigcustomer.dao.ConsultStatementTypeDao">

    <select id="listByConsultType" resultType="java.lang.String">
        SELECT
            statement_result
        FROM
            consult_statement_type
        WHERE
            consult_type = #{consultType};
    </select>
</mapper>