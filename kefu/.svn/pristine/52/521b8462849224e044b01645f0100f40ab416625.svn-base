	public String toReckonHtmlString(String type, String value) {
		
		
		DecimalFormat    df   = new DecimalFormat("######0");
		String opType = "";
		if("all".equals(type)) {
			opType = "openAllCustWindow";
		} else if("bigarea".equals(type)) {
			opType = "openBigareaCustWindow";
		} else if("province".equals(type)) {
			opType = "openProvinceCustWindow";
		} else if("city".equals(type)) {
			opType = "openCityCustWindow";
		} else if("branch".equals(type)) {
			opType = "openBranchCustWindow";
		} 
	   //设置百分数精确度2即保留两位小数
	   double customer_sum = aCustomerSum+bCustomerSum+cCustomerSum+dCustomerSum+eCustomerSum+fCustomerSum+gCustomerSum;
	   double customer_sum_two = aCustomerSumTwo+bCustomerSumTwo+cCustomerSumTwo+dCustomerSumTwo+eCustomerSumTwo+fCustomerSumTwo+gCustomerSumTwo;
	   double lost_customer_sum = bLostCustomerSum+cLostCustomerSum+dLostCustomerSum+eLostCustomerSum+fLostCustomerSum+gLostCustomerSum;
	   double add_customer_sum = bAddCustomerSum+cAddCustomerSum+dAddCustomerSum+eAddCustomerSum+fAddCustomerSum+gAddCustomerSum;
		return String
				.format("<td >%s</td><td >%s</td><td >%s</td>"
						+"<td >%s</td><td ><a href='javascript: "+opType+"(\""+value+","+"b_lost"+"\");'>%s</a></td><td ><a href='javascript: "+opType+"(\""+value+","+"b_add"+"\");'>%s</a></td>"
						+"<td >%s</td><td ><a href='javascript: "+opType+"(\""+value+","+"c_lost"+"\");'>%s</a></td><td ><a href='javascript: "+opType+"(\""+value+","+"c_add"+"\");'>%s</a></td>"
						+"<td >%s</td><td ><a href='javascript: "+opType+"(\""+value+","+"d_lost"+"\");'>%s</a></td><td ><a href='javascript: "+opType+"(\""+value+","+"d_add"+"\");'>%s</a></td>"
						+"<td >%s</td><td ><a href='javascript: "+opType+"(\""+value+","+"e_lost"+"\");'>%s</a></td><td ><a href='javascript: "+opType+"(\""+value+","+"e_add"+"\");'>%s</a></td>"
						+"<td >%s</td><td ><a href='javascript: "+opType+"(\""+value+","+"f_lost"+"\");'>%s</a></td><td ><a href='javascript: "+opType+"(\""+value+","+"f_add"+"\");'>%s</a></td>"
						+"<td >%s</td><td ><a href='javascript: "+opType+"(\""+value+","+"g_lost"+"\");'>%s</a></td><td ><a href='javascript: "+opType+"(\""+value+","+"g_add"+"\");'>%s</a></td>",

						df.format((long)add_customer_sum-(long)lost_customer_sum),df.format((long)lost_customer_sum),df.format((long)add_customer_sum),
						df.format((long)bAddCustomerSum-(long)bLostCustomerSum),df.format( bLostCustomerSum), df.format(bAddCustomerSum), 
						df.format((long)cAddCustomerSum-(long)cLostCustomerSum),df.format( cLostCustomerSum), df.format(cAddCustomerSum), 
						df.format((long)dAddCustomerSum-(long)dLostCustomerSum),df.format( dLostCustomerSum), df.format(dAddCustomerSum), 
						df.format((long)eAddCustomerSum-(long)eLostCustomerSum),df.format( eLostCustomerSum), df.format(eAddCustomerSum), 
						df.format((long)fAddCustomerSum-(long)fLostCustomerSum),df.format( fLostCustomerSum), df.format(fAddCustomerSum), 
						df.format((long)gAddCustomerSum-(long)gLostCustomerSum),df.format( gLostCustomerSum), df.format(gAddCustomerSum)); 

	}
	
	
	public ReportFluctuateDO toReckonList() {
		ReportFluctuateDO totalData =new ReportFluctuateDO();
		DecimalFormat    df   = new DecimalFormat("######0"); 
		 NumberFormat nt = NumberFormat.getPercentInstance();
	   //设置百分数精确度2即保留两位小数
	   nt.setMinimumFractionDigits(1);
	   double customer_sum = aCustomerSum+bCustomerSum+cCustomerSum+dCustomerSum+eCustomerSum+fCustomerSum+gCustomerSum;
	   double customer_sum_two = aCustomerSumTwo+bCustomerSumTwo+cCustomerSumTwo+dCustomerSumTwo+eCustomerSumTwo+fCustomerSumTwo+gCustomerSumTwo;
	   double lost_customer_sum = bLostCustomerSum+cLostCustomerSum+dLostCustomerSum+eLostCustomerSum+fLostCustomerSum+gLostCustomerSum;
	   double add_customer_sum = bAddCustomerSum+cAddCustomerSum+dAddCustomerSum+eAddCustomerSum+fAddCustomerSum+gAddCustomerSum;
	   
	   totalData.setCustomerName(customerName);
	   totalData.setBigarea(bigarea);
	   totalData.setProvinceid(provinceid);
	   totalData.setCityid(cityid);
	   totalData.setTmpXuanze(tmpXuanze);
	   totalData.setBranchCode(branchCode);
	   
	   totalData.setCzCustomerSum((long)add_customer_sum-(long)lost_customer_sum);
	   totalData.setLostCustomerSum((long)lost_customer_sum);
	   totalData.setAddCustomerSum((long)add_customer_sum);
	   
	   totalData.setCzBCustomerSum((long)bAddCustomerSum-(long)bLostCustomerSum);
	   totalData.setBLostCustomerSum(bLostCustomerSum);
	   totalData.setBAddCustomerSum(bAddCustomerSum);
	   
	   totalData.setCzCCustomerSum((long)cAddCustomerSum-(long)cLostCustomerSum);
	   totalData.setCLostCustomerSum(cLostCustomerSum);
	   totalData.setCAddCustomerSum(cAddCustomerSum);
	   
	   totalData.setCzDCustomerSum((long)dAddCustomerSum-(long)dLostCustomerSum);
	   totalData.setDLostCustomerSum(dLostCustomerSum);
	   totalData.setDAddCustomerSum(dAddCustomerSum);
	   
	   totalData.setCzECustomerSum((long)eAddCustomerSum-(long)eLostCustomerSum);
	   totalData.setELostCustomerSum(eLostCustomerSum);
	   totalData.setEAddCustomerSum(eAddCustomerSum);
	   
	   totalData.setCzFCustomerSum((long)fAddCustomerSum-(long)fLostCustomerSum);
	   totalData.setFLostCustomerSum(fLostCustomerSum);
	   totalData.setFAddCustomerSum(fAddCustomerSum);
	   
	   totalData.setCzGCustomerSum((long)gAddCustomerSum-(long)gLostCustomerSum);
	   totalData.setGLostCustomerSum(gLostCustomerSum);
	   totalData.setGAddCustomerSum(gAddCustomerSum);
	   
	   
	 return totalData;
	}
