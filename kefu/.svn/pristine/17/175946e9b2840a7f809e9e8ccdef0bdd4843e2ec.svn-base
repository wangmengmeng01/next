<!DOCTYPE html>
<html lang="zh_CN" xmlns:th="http://www.thymeleaf.org">
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="record_id" name="record_id" th:value="${costreportOrderCostChange.record_id}"  type="hidden">
							<div class="form-group">	
								<label class="col-sm-3 control-label">揽件日期：</label>
								<div class="col-sm-8">
									<input id="accountDt" name="accountDt" th:value="${costreportOrderCostChange.accountDt}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">客户编码：</label>
								<div class="col-sm-8">
									<input id="customerId" name="customerId" th:value="${costreportOrderCostChange.customerId}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">网点编码：</label>
								<div class="col-sm-8">
									<input id="branchCode" name="branchCode" th:value="${costreportOrderCostChange.branchCode}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">票数：</label>
								<div class="col-sm-8">
									<input id="orderSum" name="orderSum" th:value="${costreportOrderCostChange.orderSum}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">总重量：</label>
								<div class="col-sm-8">
									<input id="weightAll" name="weightAll" th:value="${costreportOrderCostChange.weightAll}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">收入：</label>
								<div class="col-sm-8">
									<input id="income" name="income" th:value="${costreportOrderCostChange.income}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">调整：</label>
								<div class="col-sm-8">
									<input id="changeIncome" name="changeIncome" th:value="${costreportOrderCostChange.changeIncome}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">实收：</label>
								<div class="col-sm-8">
									<input id="actualIncome" name="actualIncome" th:value="${costreportOrderCostChange.actualIncome}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">已收款金额：</label>
								<div class="col-sm-8">
									<input id="finishIncome" name="finishIncome" th:value="${costreportOrderCostChange.finishIncome}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">is_finish：</label>
								<div class="col-sm-8">
									<input id="isFinish" name="isFinish" th:value="${costreportOrderCostChange.isFinish}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">creater：</label>
								<div class="col-sm-8">
									<input id="creater" name="creater" th:value="${costreportOrderCostChange.creater}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">creater_branch：</label>
								<div class="col-sm-8">
									<input id="createrBranch" name="createrBranch" th:value="${costreportOrderCostChange.createrBranch}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">create_time：</label>
								<div class="col-sm-8">
									<input id="createTime" name="createTime" th:value="${costreportOrderCostChange.createTime}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">editer：</label>
								<div class="col-sm-8">
									<input id="editer" name="editer" th:value="${costreportOrderCostChange.editer}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">editer_branch：</label>
								<div class="col-sm-8">
									<input id="editerBranch" name="editerBranch" th:value="${costreportOrderCostChange.editerBranch}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">edit_time：</label>
								<div class="col-sm-8">
									<input id="editTime" name="editTime" th:value="${costreportOrderCostChange.editTime}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">揽件重量：</label>
								<div class="col-sm-8">
									<input id="senderWeight" name="senderWeight" th:value="${costreportOrderCostChange.senderWeight}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">T+3重量：</label>
								<div class="col-sm-8">
									<input id="t3Weight" name="t3Weight" th:value="${costreportOrderCostChange.t3Weight}" class="form-control" type="text">
								</div>
							</div>
																					<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" th:src="@{/js/appjs/feiniao/costreport/costreportOrderCostChange/edit.js}">
	</script>
</body>
</html>
