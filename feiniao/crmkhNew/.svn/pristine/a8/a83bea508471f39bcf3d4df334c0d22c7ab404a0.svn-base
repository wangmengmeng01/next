<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yunda.base.system.dao.FileExportDao">

	<select id="get" resultType="com.yunda.base.system.domain.FileExportDO">
		select `id`,`user_id`,`create_time`,`start_time`,`end_time`,`hand_count`,`execute_class`,`execute_method`,`execute_param`,`spec_class`,`title`,`state`,`file_path` from crmkh_file_export where id = #{value}
	</select>
<select id="getRepeatData" resultType="com.yunda.base.system.domain.FileExportDO">
		select
		`id`,`user_id`,`create_time`,`end_time`,`execute_class`,`execute_method`,`execute_param`,`title`,`state`,`file_path`
		from crmkh_file_export
		<where>
			<if test="userId != null and userId != ''"> and user_id = #{userId} </if>
			<if test="executeClass != null and executeClass != ''"> and execute_class = #{executeClass} </if>
			<if test="executeMethod != null and executeMethod != ''"> and execute_method = #{executeMethod} </if>
			<if test="executeParam != null and executeParam != ''"> and execute_param = #{executeParam} </if>
		</where>
	</select>
	<select id="getTitle" resultType="java.lang.String">
		select
	    distinct `title`
		from crmkh_file_export
		<where>
			<if test="title != null and title != ''"> and title like concat(concat('%',#{title}),'%') </if>
		</where>
	</select>
	<select id="getWaitingTask" resultType="com.yunda.base.system.domain.FileExportDO">
		select
		`id`,`user_id`,`create_time`,`end_time`,`spec_class`,`execute_class`,`execute_method`,`execute_param`,`title`,`state`,`file_path`
		from crmkh_file_export
		<where>
			<if test="state != null and state != ''"> and state = #{state} </if>
		</where>
	</select>
	<select id="list" resultType="com.yunda.base.system.domain.FileExportDO">
		select `id`,`user_id`,`create_time`,`start_time`,`end_time`,`hand_count`,`execute_class`,`execute_method`,`execute_param`,`spec_class`,`title`,`state`,`file_path` from crmkh_file_export
        <where>  
		  		  <if test="id != null and id != ''"> and id = #{id} </if>
		  		  <if test="userId != null and userId != ''"> and user_id = #{userId} </if>
		  		  <if test="createTime != null and createTime != ''"> and create_time = #{createTime} </if>
		  		  <if test="startTime != null and startTime != ''"> and start_time = #{startTime} </if>
		  		  <if test="endTime != null and endTime != ''"> and end_time = #{endTime} </if>
		  		  <if test="handCount != null and handCount != ''"> and hand_count = #{handCount} </if>
		  		  <if test="executeClass != null and executeClass != ''"> and execute_class = #{executeClass} </if>
		  		  <if test="executeMethod != null and executeMethod != ''"> and execute_method = #{executeMethod} </if>
		  		  <if test="executeParam != null and executeParam != ''"> and execute_param = #{executeParam} </if>
		  		  <if test="specClass != null and specClass != ''"> and spec_class = #{specClass} </if>
		  		  <if test="title != null and title != ''"> and title = #{title} </if>
		  		  <if test="state != null and state != ''"> and state = #{state} </if>
		  		  <if test="filePath != null and filePath != ''"> and file_path = #{filePath} </if>
		  		</where>
        <choose>
            <when test="sort != null and sort.trim() != ''">
                order by #{sort}, #{order}
            </when>
			<otherwise>
                order by id desc
			</otherwise>
        </choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="count" resultType="int">
		select count(*) from crmkh_file_export
		 <where>  
		  		  <if test="id != null and id != ''"> and id = #{id} </if>
		  		  <if test="userId != null and userId != ''"> and user_id = #{userId} </if>
		  		  <if test="createTime != null and createTime != ''"> and create_time = #{createTime} </if>
		  		  <if test="startTime != null and startTime != ''"> and start_time = #{startTime} </if>
		  		  <if test="endTime != null and endTime != ''"> and end_time = #{endTime} </if>
		  		  <if test="handCount != null and handCount != ''"> and hand_count = #{handCount} </if>
		  		  <if test="executeClass != null and executeClass != ''"> and execute_class = #{executeClass} </if>
		  		  <if test="executeMethod != null and executeMethod != ''"> and execute_method = #{executeMethod} </if>
		  		  <if test="executeParam != null and executeParam != ''"> and execute_param = #{executeParam} </if>
		  		  <if test="specClass != null and specClass != ''"> and spec_class = #{specClass} </if>
		  		  <if test="title != null and title != ''"> and title = #{title} </if>
		  		  <if test="state != null and state != ''"> and state = #{state} </if>
		  		  <if test="filePath != null and filePath != ''"> and file_path = #{filePath} </if>
		  		</where>
	</select>
	 
	<insert id="save" parameterType="com.yunda.base.system.domain.FileExportDO" useGeneratedKeys="true" keyProperty="id">
		insert into crmkh_file_export
		(
			`user_id`, 
			`create_time`, 
			`start_time`, 
			`end_time`, 
			`hand_count`, 
			`execute_class`, 
			`execute_method`, 
			`execute_param`, 
			`spec_class`, 
			`title`, 
			`state`, 
			`file_path`
		)
		values
		(
			#{userId}, 
			#{createTime}, 
			#{startTime}, 
			#{endTime}, 
			#{handCount}, 
			#{executeClass}, 
			#{executeMethod}, 
			#{executeParam}, 
			#{specClass}, 
			#{title}, 
			#{state}, 
			#{filePath}
		)
	</insert>
	 
	<update id="update" parameterType="com.yunda.base.system.domain.FileExportDO">
		update crmkh_file_export 
		<set>
			<if test="userId != null">`user_id` = #{userId}, </if>
			<if test="createTime != null">`create_time` = #{createTime}, </if>
			<if test="startTime != null">`start_time` = #{startTime}, </if>
			<if test="endTime != null">`end_time` = #{endTime}, </if>
			<if test="handCount != null">`hand_count` = #{handCount}, </if>
			<if test="executeClass != null">`execute_class` = #{executeClass}, </if>
			<if test="executeMethod != null">`execute_method` = #{executeMethod}, </if>
			<if test="executeParam != null">`execute_param` = #{executeParam}, </if>
			<if test="specClass != null">`spec_class` = #{specClass}, </if>
			<if test="title != null">`title` = #{title}, </if>
			<if test="state != null">`state` = #{state}, </if>
			<if test="filePath != null">`file_path` = #{filePath}</if>
		</set>
		where id = #{id}
	</update>
	
	<delete id="remove">
		delete from crmkh_file_export where id = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from crmkh_file_export where id in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>