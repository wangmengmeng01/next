package com.yunda.base.feiniao.warning.bo;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

import com.yunda.base.common.bo.Bo_Interface;

@ApiModel(value="预警基准设置")
public class Bo_warningBenchmark implements Bo_Interface {

	@ApiModelProperty(value="修改时间")
	public String updateTime;
	
	@ApiModelProperty(value="每页查询数")
	public int limit;
	
	@ApiModelProperty(value="每页开始位置")
	public int offSet;
	
	@ApiModelProperty(value="机构名称")
	public String jgmc;

	public String getUpdateTime() {
		return updateTime;
	}

	public void setUpdateTime(String updateTime) {
		this.updateTime = updateTime;
	}

	public int getLimit() {
		return limit;
	}

	public void setLimit(int limit) {
		this.limit = limit;
	}

	public int getOffSet() {
		return offSet;
	}

	public void setOffSet(int offSet) {
		this.offSet = offSet;
	}

	public String getJgmc() {
		return jgmc;
	}

	public void setJgmc(String jgmc) {
		this.jgmc = jgmc;
	}
	
	
	
}
