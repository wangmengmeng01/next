package com.yunda.base.system.vo;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

import java.io.Serializable;

/** 
 * @ClassName: ValidParam 
 * @Description: TODO
 * @author: 22374
 * @date: 2019年2月18日 下午1:20:37  
 */
@ApiModel("资源配置页面验证参数")
public class ValidParam implements Serializable {
	private static final long serialVersionUID = -1343540377513066047L;
	@ApiModelProperty("身份证")
	private String id_card;
	@ApiModelProperty("短信验证码")
	private String message_valid;
	@ApiModelProperty("用户名")
	private String username;
	@ApiModelProperty("用户图像")
	private String base;
	@ApiModelProperty("mac地址")
	private String mac;
	

	/**
	 * @return the id_card
	 */
	public String getId_card() {
		return id_card;
	}

	/**
	 * @param id_card the id_card to set
	 */
	public void setId_card(String id_card) {
		this.id_card = id_card;
	}

	/**
	 * @return the message_valid
	 */
	public String getMessage_valid() {
		return message_valid;
	}

	/**
	 * @param message_valid the message_valid to set
	 */
	public void setMessage_valid(String message_valid) {
		this.message_valid = message_valid;
	}

	/**
	 * @return the username
	 */
	public String getUsername() {
		return username;
	}

	/**
	 * @param username the username to set
	 */
	public void setUsername(String username) {
		this.username = username;
	}

	/**
	 * @return the base
	 */
	public String getBase() {
		return base;
	}

	/**
	 * @param base the base to set
	 */
	public void setBase(String base) {
		this.base = base;
	}

	/**
	 * @return the mac
	 */
	public String getMac() {
		return mac;
	}

	/**
	 * @param mac the mac to set
	 */
	public void setMac(String mac) {
		this.mac = mac;
	}

}
