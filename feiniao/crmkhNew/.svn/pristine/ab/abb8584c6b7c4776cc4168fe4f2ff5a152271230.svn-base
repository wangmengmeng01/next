<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
	namespace="com.yunda.base.feiniao.schedule.suckdata.dao.RecordSuckDao">

	<select id="get"
		resultType="com.yunda.base.feiniao.schedule.suckdata.domain.RecordSuckDO">
		select
		`id`,`suck_date`,`creat_time`,`gp_nums`,`del_flag` from
		crmkhv2_record_suck where id = #{value}
	</select>

	<select id="list"
		resultType="com.yunda.base.feiniao.schedule.suckdata.domain.RecordSuckDO">
		select
		`id`,`suck_date`,`creat_time`,`gp_nums`,`del_flag` from
		crmkhv2_record_suck
		<where>
			<if test="id != null and id != ''"> and id = #{id} </if>
			<if test="suckDate != null and suckDate != ''"> and suck_date = #{suckDate} </if>
			<if test="creatTime != null and creatTime != ''"> and creat_time = #{creatTime} </if>
			<if test="gpNums != null and gpNums != ''"> and gp_nums = #{gpNums} </if>
			<if test="delFlag != null and delFlag != ''"> and del_flag = #{delFlag} </if>
		</where>
		<choose>
			<when test="sort != null and sort.trim() != ''">
				order by #{sort} ,#{order}
			</when>
			<otherwise>
				order by id desc
			</otherwise>
		</choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>

	<select id="count" resultType="int">
		select count(*) from crmkhv2_record_suck
		<where>
			<if test="id != null and id != ''"> and id = #{id} </if>
			<if test="suckDate != null and suckDate != ''"> and suck_date = #{suckDate} </if>
			<if test="creatTime != null and creatTime != ''"> and creat_time = #{creatTime} </if>
			<if test="gpNums != null and gpNums != ''"> and gp_nums = #{gpNums} </if>
			<if test="delFlag != null and delFlag != ''"> and del_flag = #{delFlag} </if>
		</where>
	</select>

	<insert id="save"
		parameterType="com.yunda.base.feiniao.schedule.suckdata.domain.RecordSuckDO"
		useGeneratedKeys="true" keyProperty="id">
		insert into crmkhv2_record_suck
		(
		`suck_date`,
		`gp_nums`,
		`del_flag`
		)
		values
		(
		#{suckDate},
		#{gpNums},
		#{delFlag}
		)
	</insert>

	<update id="update"
		parameterType="com.yunda.base.feiniao.schedule.suckdata.domain.RecordSuckDO">
		update crmkhv2_record_suck
		<set>
			<if test="suckDate != null">`suck_date` = #{suckDate}, </if>
			<if test="gpNums != null">`gp_nums` = #{gpNums}, </if>
			<if test="delFlag != null">`del_flag` = #{delFlag}</if>
		</set>
		where id = #{id}
	</update>

	<update id="delMark"
		parameterType="com.yunda.base.feiniao.schedule.suckdata.domain.RecordSuckDO">
		update crmkhv2_record_suck set del_flag = -1
		<where>
			<if test="id != null and id != ''"> and id = #{id} </if>
			<if test="suckDate != null and suckDate != ''"> and suck_date = #{suckDate} </if>
		</where>
	</update>

	<delete id="remove">
		delete from crmkhv2_record_suck where id = #{value}
	</delete>

	<delete id="batchRemove">
		delete from crmkhv2_record_suck where id in
		<foreach item="id" collection="array" open="(" separator=","
			close=")">
			#{id}
		</foreach>
	</delete>

</mapper>