	   double customer_sum_two = a_customer_sum_two+b_customer_sum_two+c_customer_sum_two+d_customer_sum_two+e_customer_sum_two+f_customer_sum_two+g_customer_sum_two;
	   double lost_customer_sum = b_lost_customer_sum+c_lost_customer_sum+d_lost_customer_sum+e_lost_customer_sum+f_lost_customer_sum+g_lost_customer_sum;
	   double add_customer_sum = b_add_customer_sum+c_add_customer_sum+d_add_customer_sum+e_add_customer_sum+f_add_customer_sum+g_add_customer_sum;
	   
	   totalData.setCustomer_name(customer_name);
	   totalData.setBigarea(bigarea);
	   totalData.setProvinceID(ProvinceID);
	   totalData.setCityID(CityID);
	   totalData.setTmp_xuanze(tmp_xuanze);
	   
	   totalData.setCz_customer_sum((long)add_customer_sum-(long)lost_customer_sum);
	   totalData.setLost_customer_sum((long)lost_customer_sum);
	   totalData.setAdd_customer_sum((long)add_customer_sum);
	   
	   totalData.setCz_b_customer_sum((long)b_add_customer_sum-(long)b_lost_customer_sum);
	   totalData.setB_lost_customer_sum(b_lost_customer_sum);
	   totalData.setB_add_customer_sum(b_add_customer_sum);
	   
	   totalData.setCz_c_customer_sum((long)c_add_customer_sum-(long)c_lost_customer_sum);
	   totalData.setC_lost_customer_sum(c_lost_customer_sum);
	   totalData.setC_add_customer_sum(c_add_customer_sum);
	   
	   totalData.setCz_d_customer_sum((long)d_add_customer_sum-(long)d_lost_customer_sum);
	   totalData.setD_lost_customer_sum(d_lost_customer_sum);
	   totalData.setD_add_customer_sum(d_add_customer_sum);
	   
	   totalData.setCz_e_customer_sum((long)e_add_customer_sum-(long)e_lost_customer_sum);
	   totalData.setE_lost_customer_sum(e_lost_customer_sum);
	   totalData.setE_add_customer_sum(e_add_customer_sum);
	   
	   totalData.setCz_f_customer_sum((long)f_add_customer_sum-(long)f_lost_customer_sum);
	   totalData.setF_lost_customer_sum(f_lost_customer_sum);
	   totalData.setF_add_customer_sum(f_add_customer_sum);
	   
	   totalData.setCz_g_customer_sum((long)g_add_customer_sum-(long)g_lost_customer_sum);
	   totalData.setG_lost_customer_sum(g_lost_customer_sum);
	   totalData.setG_add_customer_sum(g_add_customer_sum);
	   
	   
	 return totalData;
	}
	
